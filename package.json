{
  "name": "@blueagle/readify",
  "version": "0.0.1",
  "devDependencies": {
    "@types/bun": "1.1.13",
    "@types/chrome": "0.0.280",
    "@types/node": "22.9.0",
    "eslint": "^9.14.0",
    "eslint-plugin-perfectionist": "^3.9.1",
    "prettier": "3.3.3",
    "typescript": "5.8.0-dev.20241106",
    "typescript-eslint": "8.13.1-alpha.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "bun link gcc-ts-bundler && bun gcc-ts-bundler --src-dir='./src' --entry_point='./index.ts' --js_output_file='./dist/index.js' --language_out=ES5",
    "lint": "bun --bun prettier \"src/*.ts\" --write && bun --bun eslint \"src/*.ts\" --fix"
  },
  "sideEffects": [],
  "type": "module"
}